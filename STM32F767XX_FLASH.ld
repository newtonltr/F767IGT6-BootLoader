/* Entry Point */
ENTRY(Reset_Handler)

/* Highest address of the user mode stack */
_estack = ORIGIN(DTCM_RAM) + 512K; /* end of "RAM" Ram type memory */

_Min_Heap_Size = 0x200; /* required amount of heap */
_Min_Stack_Size = 0x6000; /* required amount of stack */
_Rx_Desc_Addr = 0x7c000;
_Tx_Desc_Addr = 0x7c0a0;

/* Memories definition */
/* SRAM TOTAL 512K */
MEMORY
{
  DTCM_RAM  (xrw)    : ORIGIN = 0x20000000,   LENGTH = 128k
  SRAM1     (xrw)    : ORIGIN = 0x20020000,   LENGTH = 368k
  SRAM2     (xrw)    : ORIGIN = 0x2007C000,   LENGTH = 16K
  EXTSRAM   (xrw)    : ORIGIN = 0x60000000,   LENGTH = 1024K
  FLASH     (rx)     : ORIGIN = 0x8000000,    LENGTH = 1024K
  /*EXTFLASH    (rx)    : ORIGIN = 0x90000000,   LENGTH = 16384K*/
}

/* Sections */
SECTIONS
{
  /* The startup code into "FLASH" Rom type memory */
  .isr_vector :
  {
    . = ALIGN(4);
    KEEP(*(.isr_vector)) /* Startup code */
    . = ALIGN(4);
  } >FLASH

  /* The program code and other data into "FLASH" Rom type memory */
  .text :
  {
    . = ALIGN(4);
    *(.text)           /* .text sections (code) */
    *(.text*)          /* .text* sections (code) */
    *(.glue_7)         /* glue arm to thumb code */
    *(.glue_7t)        /* glue thumb to arm code */
    *(.eh_frame)

    KEEP (*(.init))
    KEEP (*(.fini))

    . = ALIGN(4);
    _etext = .;        /* define a global symbols at end of code */
  } >FLASH

  /* Constant data into "FLASH" Rom type memory */
  .rodata :
  {
    . = ALIGN(4);
    *(.rodata)         /* .rodata sections (constants, strings, etc.) */
    *(.rodata*)        /* .rodata* sections (constants, strings, etc.) */
    . = ALIGN(4);
  } >FLASH

  .ARM.extab   : {
    . = ALIGN(4);
    *(.ARM.extab* .gnu.linkonce.armextab.*)
    . = ALIGN(4);
  } >FLASH

  .ARM : {
    . = ALIGN(4);
    __exidx_start = .;
    *(.ARM.exidx*)
    __exidx_end = .;
    . = ALIGN(4);
  } >FLASH

  .preinit_array     :
  {
    . = ALIGN(4);
    PROVIDE_HIDDEN (__preinit_array_start = .);
    KEEP (*(.preinit_array*))
    PROVIDE_HIDDEN (__preinit_array_end = .);
    . = ALIGN(4);
  } >FLASH

  .init_array :
  {
    . = ALIGN(4);
    PROVIDE_HIDDEN (__init_array_start = .);
    KEEP (*(SORT(.init_array.*)))
    KEEP (*(.init_array*))
    PROVIDE_HIDDEN (__init_array_end = .);
    . = ALIGN(4);
  } >FLASH

  .fini_array :
  {
    . = ALIGN(4);
    PROVIDE_HIDDEN (__fini_array_start = .);
    KEEP (*(SORT(.fini_array.*)))
    KEEP (*(.fini_array*))
    PROVIDE_HIDDEN (__fini_array_end = .);
    . = ALIGN(4);
  } >FLASH

  /* Used by the startup to initialize data */
  _sidata = LOADADDR(.data);

  /* Initialized data sections into "RAM" Ram type memory */
  .data :
  {
    . = ALIGN(4);
    _sdata = .;        /* create a global symbol at data start */
    *(.data)           /* .data sections */
    *(.data*)          /* .data* sections */
    *(.RamFunc)        /* .RamFunc sections */
    *(.RamFunc*)       /* .RamFunc* sections */

    . = ALIGN(4);
    _edata = .;        /* define a global symbol at data end */

  } >SRAM1 AT> FLASH

  /* Uninitialized data section into "RAM" Ram type memory */
  . = ALIGN(4);
  .bss :
  {
    /* This is used by the startup in order to initialize the .bss section */
    _sbss = .;         /* define a global symbol at bss start */
    __bss_start__ = _sbss;
    *(.bss)
    *(.bss*)
    *(COMMON)

    . = ALIGN(4);
    _ebss = .;         /* define a global symbol at bss end */
    __bss_end__ = _ebss;
  } >SRAM1

  /* User_heap_stack section, used to check that there is enough "RAM" Ram  type memory left */
  ._user_heap_stack :
  {
    . = ALIGN(8);
    PROVIDE ( end = . );
    PROVIDE ( _end = . );
    . = . + _Min_Heap_Size;
    . = . + _Min_Stack_Size;
    . = ALIGN(8);
  } >SRAM1

  /* RxDecripSection and TxDecripSection now allocated in SRAM2 */
  .RxDecripSection 0x2007C000 (NOLOAD):
  {
    . = ALIGN(4);
    __RxDecripSection_Start = .;
    *(.RxDecripSection);
    . = ALIGN(4);
    __RxDecripSection_End = .;
  } >SRAM2 AT> FLASH /* 使用 SRAM2 区域 */

  .TxDecripSection 0x2007C100 (NOLOAD):
  {
    . = ALIGN(4);
    __TxDecripSection_Start = .;
    *(.TxDecripSection);
    . = ALIGN(4);
    __TxDecripSection_End = .;
  } >SRAM2 AT> FLASH /* 使用 SRAM2 区域 */

  .NxPoolSection 0x2007C200 (NOLOAD):
  {
    . = ALIGN(4);
    __NxPoolSection_Start = .;
    *(.NxPoolSection);
    . = ALIGN(4);
    __NxPoolSection_End = .;
  } >SRAM2 AT> FLASH /* 使用 SRAM2 区域 */

	__RAM_segment_used_end__ = .;

  /* Remove information from the compiler libraries */
  /DISCARD/ :
